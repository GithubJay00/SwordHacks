	LungeBotRange = 5 , 
	
	
	AimbotToggled = nil , 
	AimbotSliderValue = 5 ,
	AimbotTargetPart = "Torso" ,
	AimbotOffset = 0 ,
	
	
	JumpbotToggle = nil , 
	JumpbotRange = 5
	
}



