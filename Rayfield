local VisualP = Instance.new("Part")
VisualP.Anchored = true
VisualP.CanCollide = false
VisualP.CastShadow = false
VisualP.Transparency = 0.9
VisualP.Material = Enum.Material.Neon
VisualP.Shape = "Ball"



